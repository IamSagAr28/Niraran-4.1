{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --force",
  "rewrites": [
    {
      "source": "/auth/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}